{
  "api_base_url": "http://localhost:5000/api",
  "frontend_url": "http://localhost:3000",
  "endpoints": [
    "POST /api/auth/register",
    "POST /api/auth/login",
    "GET /api/auth/me",
    "GET /health"
  ],
  "cors_config": {
    "allowed_origins": [
      "http://localhost:3000"
    ],
    "allowed_methods": [
      "GET",
      "POST",
      "PUT",
      "DELETE"
    ],
    "allowed_headers": [
      "Content-Type",
      "Authorization"
    ]
  },
  "authentication": {
    "type": "JWT",
    "header": "Authorization",
    "prefix": "Bearer"
  }
}